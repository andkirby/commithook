{
  "name": "andkirby/commithook",
  "description": "PHP Commit Hooks",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Andrew Roslik",
      "email": "bizkirby@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/console": "2.*||3.*"
  },
  "require-dev": {
    "php": ">=5.3.2",
    "symfony/console": "2.*||3.*"
  },
  "suggest": {
    "phpunit/phpunit": "4.*"
  },
  "bin": [
    "bin/commithook"
  ],
  "autoload": {
    "psr-4": {
      "PreCommit\\": "LibHooks/lib/PreCommit",
      "PreCommit\\Test\\": "LibHooks/tests/testsuite/PreCommit/Test"
    }
  }
